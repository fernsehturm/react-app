{
  "name": "@fernsehturm/react-app",
  "version": "0.0.1",
  "description": "Basic components of a webapp in a full stack",
  "main": "dist/index.js",
  "scripts": {
    "typegen": "tsc",
    "semantic-release": "semantic-release",
    "test": "jest",
    "lint": "eslint ./src -c .eslintrc.js",
    "lint:fix": "eslint ./src -c .eslintrc.js --fix",
    "build": "rimraf dist && tsc && babel ./src --out-dir dist --extensions .ts,.tsx",
    "example-app": "npm config set script-shell \"/bin/bash\";bash start_example.sh DEV_APP_PORT",
    "example-web": "npm config set script-shell \"/bin/bash\";bash start_example.sh DEV_WEB_PORT"
  },
  "author": "Dr. Frank Zickert",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.23.0",
    "@babel/core": "^7.23.3",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.23.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/preset-env": "^7.23.3",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/gitlab": "^12.1.0",
    "@semantic-release/npm": "^11.0.1",
    "@stylistic/eslint-plugin": "^1.3.1",
    "@types/react": "^18.2.37",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "axios": "^1.6.2",
    "babel-loader": "^9.1.3",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-plugin-transform-async-to-promises": "^0.8.18",
    "eslint": "^8.53.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^5.0.1",
    "html-webpack-plugin": "^5.5.3",
    "murmurhash3js": "^3.0.1",
    "prettier": "^3.1.0",
    "prettier-eslint": "^16.1.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^6.19.0",
    "rimraf": "^5.0.5",
    "semantic-release": "^22.0.8",
    "typescript": "^5.2.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  }
}
